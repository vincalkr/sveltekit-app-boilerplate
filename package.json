{
	"name": "sveltekit-app-boilerplate",
	"version": "1.0.0",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"package": "svelte-kit package",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"prisma": {
		"seed": "node --no-warnings --loader ts-node/esm prisma/seed.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.72",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.0",
		"@types/jsonwebtoken": "^8.5.8",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"argon2": "^0.28.5",
		"autoprefixer": "^10.4.0",
		"broadcast-channel": "^4.11.0",
		"cookie": "^0.5.0",
		"cssnano": "^5.0.10",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"jsonwebtoken": "^8.5.1",
		"postcss": "^8.4.4",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"prisma": "^3.10.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"vite-imagetools": "^3.4.0-next.7"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.15.4",
		"@prisma/client": "^3.10.0",
		"@sveltestack/svelte-query": "^1.4.1 ",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/typography": "^0.5.0",
		"@trpc/client": "^9.22.0",
		"@trpc/server": "^9.22.0",
		"axios": "^0.26.1",
		"decimal.js": "^10.3.1",
		"superjson": "^1.8.1",
		"svelte-forms-lib": "^2.0.1",
		"trpc-sveltekit": "^1.2.3",
		"zod": "^3.14.4"
	}
}
